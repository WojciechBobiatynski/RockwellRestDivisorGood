<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" updateCheck="false">

    <defaultCache eternal="false" timeToIdleSeconds="0"
        timeToLiveSeconds="3600"
        maxElementsInMemory="1000" overflowToDisk="false" />

    <cache name="dictionaryList" eternal="false"
        timeToIdleSeconds="0" timeToLiveSeconds="3600"
        maxElementsInMemory="1000"
        overflowToDisk="false" />

    <cache name="contactTypeList" eternal="false"
        timeToIdleSeconds="0" timeToLiveSeconds="3600"
        maxElementsInMemory="1000"
        overflowToDisk="false" />

    <cache name="statesInPolandList" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="3600"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="enterpriseList" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="3600"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="elctRmbsStatuses" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="3600"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="trainingInstanceStatuses" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="3600"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="elctRmbsTypes" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="3600"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <!-- Pattern Service -->
    <cache name="ExternalOrderIdPatternServiceImpl.getPattern" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="86400"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="ExternalOrderIdPatternServiceImpl.getPatternCode" eternal="false"
        timeToIdleSeconds="0" timeToLiveSeconds="86400"
        maxElementsInMemory="1000"
        overflowToDisk="false" />

    <cache name="ImportTrainingPatternServiceImpl.getPattern" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="86400"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

    <cache name="ImportTrainingPatternServiceImpl.getPatternCode" eternal="false"
           timeToIdleSeconds="0" timeToLiveSeconds="86400"
           maxElementsInMemory="1000"
           overflowToDisk="false" />

</ehcache>
